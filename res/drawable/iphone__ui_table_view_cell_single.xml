<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
		<shape android:shape="rectangle">
		    <corners
		        android:radius="7sp"/>
		    <padding
		        android:left="8sp"
		        android:right="8sp"
		        android:top="4sp"
		        android:bottom="4sp"
		    />
		    		    <stroke
		        android:width="1sp"
		        android:color="@color/iphone__ui_table_cell_stroke"
		    />
			<solid android:color="@color/iphone__ui_table_cell_selected" />
		</shape>
    </item>
	<item>
		<shape android:shape="rectangle">
		    <corners
		        android:radius="7sp"/>
		    <padding
		        android:left="8sp"
		        android:right="8sp"
		        android:top="4sp"
		        android:bottom="4sp"
		    />
		    <stroke
		        android:width="1sp"
		        android:color="@color/iphone__ui_table_cell_stroke"
		    />
			<solid android:color="#ffffffff"/>
		</shape>
	</item>
</selector>
